isbn = {0969-9961 (Print)$\backslash$r0969-9961 (Linking)},
issn = {09699961},
journal = {Neurobiology of Disease},
keywords = {Aggregation,Alzheimer's disease,Atomic force microscope,Beta-amyloid,Cytotoxicity,Oligomers,Trehalose},
number = {1},
