title = {{Impaired proteolysis underlies autophagic dysfunction in Niemann-Pick type C disease.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22872701 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3607480},
volume = {21},
year = {2012}
}
