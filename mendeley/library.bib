pmid = {24398425},
title = {{Alzheimer disease: epidemiology, diagnostic criteria, risk factors and biomarkers.}},
url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC3992261/pdf/nihms562191.pdf http://www.ncbi.nlm.nih.gov/pubmed/24398425{\%}0Ahttp://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3992261},
