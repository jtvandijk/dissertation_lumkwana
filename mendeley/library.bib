title = {{Long-term oral lithium treatment attenuates motor disturbance in tauopathy model mice: Implications of autophagy promotion}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22249108 http://linkinghub.elsevier.com/retrieve/pii/S0969996112000058},
volume = {46},
year = {2012}
}
@article{Thumm1994,
abstract = {Protein degradation in the vacuole (lysosome) is an important event in cellular regulation. In yeast, as in mammalian cells, a major route of protein uptake for degradation into the vacuole (lysosome) has been found to be autophagocytosis. The discovery of this process in yeast enables the elucidation of its mechanisms via genetic and molecular biological investigations. Here we report the isolation of yeast mutants defective in autophagocytosis (aut mutants), using a rapid colony screening procedure.},
