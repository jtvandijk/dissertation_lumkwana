journal = {Journal of Cell Biology},
month = {nov},
number = {4},
pages = {603--614},
title = {{p62/SQSTM1 forms protein aggregates degraded by autophagy and has a protective effect on huntingtin-induced cell death}},
volume = {171},
year = {2005}
}
