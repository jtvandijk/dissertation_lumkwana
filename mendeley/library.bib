}
@article{Rose2010,
abstract = {Huntington's disease (HD) is an autosomal dominant neurodegenerative disease caused by a polyglutamine expansion in huntingtin. There are no treatments that are known to slow the neurodegeneration caused by this mutation. Mutant huntingtin causes disease via a toxic gain-of-function mechanism and has the propensity to aggregate and form intraneuronal inclusions. One therapeutic approach for HD is to enhance the degradation of the mutant protein. We have shown that this can be achieved by upregulating autophagy, using the drug rapamycin. In order to find safer ways of inducing autophagy for clinical purposes, we previously screened United States Food and Drug Administration-approved drugs for their autophagy-stimulating potential. This screen suggested that rilmenidine, a well tolerated, safe, centrally acting anti-hypertensive drug, could induce autophagy in cell culture via a pathway that was independent of the mammalian target of rapamycin. Here we have shown that rilmenidine induces autophagy in mice and in primary neuronal culture. Rilmenidine administration attenuated the signs of disease in a HD mouse model and reduced levels of the mutant huntingtin fragment. As rilmenidine has a long safety record and is designed for chronic use, our data suggests that it should be considered for the treatment of HD and related conditions.},
author = {Rose, Claudia and Menzies, Fiona M. and Renna, Maurizio and Acevedo-Arozena, Abraham and Corrochano, Silvia and Sadiq, Oana and Brown, Steve D. and Rubinsztein, David C.},
doi = {10.1093/hmg/ddq093},
file = {:Users/Dumi/Library/Application Support/Mendeley Desktop/Downloaded/Rose et al. - 2010 - Rilmenidine attenuates toxicity of polyglutamine expansions in a mouse model of Huntington's disease.pdf:pdf},
isbn = {1460-2083 (Electronic)$\backslash$n0964-6906 (Linking)},
issn = {09646906},
journal = {Human Molecular Genetics},
number = {11},
pages = {2144--2153},
pmid = {20190273},
title = {{Rilmenidine attenuates toxicity of polyglutamine expansions in a mouse model of Huntington's disease}},
volume = {19},
year = {2010}
}
@article{Gonzalez-Polo2015,
abstract = {The pathogenesis of neurodegenerative diseases involves altered activity of proteolytic systems and accumulation of protein aggregates. Autophagy is an intracellular process in which damaged organelles and long-lived proteins are degraded and recycled for maintaining normal cellular homeostasis. Disruption of autophagic activity in neurons leads to modify the cellular homeostasis, causing deficient elimination of abnormal and toxic protein aggregates that promotes cellular stress and death. Therefore, induction of autophagy has been proposed as a reasonable strategy to help neurons to clear abnormal protein aggregates and survive. This review aims to give an overview of some of the main modulators of autophagy that are currently being studied as possible alternatives in the search of therapies that slow the progression of neurodegenerative diseases, which are incurable to date.},
author = {Gonz{\'{a}}lez-Polo, Rosa Ana and Pizarro-Estrella, Elisa and Yakhine-Diop, Sokhna M S and Rodr{\'{i}}guez-Arribas, Mario and G{\'{o}}mez-S{\'{a}}nchez, Rub{\'{e}}n and Pedro, Jos{\'{e}} M Bravo-San and Fuentes, Jos{\'{e}} M},
issn = {1873-4294},
journal = {Current topics in medicinal chemistry},
number = {21},
pages = {2152--74},
pmid = {26059359},
title = {{Is the Modulation of Autophagy the Future in the Treatment of Neurodegenerative Diseases?}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26059359},
volume = {15},
year = {2015}
}
@article{Ravikumar2004,
abstract = {Huntington disease is one of nine inherited neurodegenerative disorders caused by a polyglutamine tract expansion. Expanded polyglutamine proteins accumulate abnormally in intracellular aggregates. Here we show that mammalian target of rapamycin (mTOR) is sequestered in polyglutamine aggregates in cell models, transgenic mice and human brains. Sequestration of mTOR impairs its kinase activity and induces autophagy, a key clearance pathway for mutant huntingtin fragments. This protects against polyglutamine toxicity, as the specific mTOR inhibitor rapamycin attenuates huntingtin accumulation and cell death in cell models of Huntington disease, and inhibition of autophagy has the converse effects. Furthermore, rapamycin protects against neurodegeneration in a fly model of Huntington disease, and the rapamycin analog CCI-779 improved performance on four different behavioral tasks and decreased aggregate formation in a mouse model of Huntington disease. Our data provide proof-of-principle for the potential of inducing autophagy to treat Huntington disease.},
author = {Ravikumar, Brinda and Vacher, Coralie and Berger, Zdenek and Davies, Janet E and Luo, Shouqing and Oroz, Lourdes G and Scaravilli, Francesco and Easton, Douglas F and Duden, Rainer and O'Kane, Cahir J and Rubinsztein, David C},
doi = {10.1038/ng1362},
file = {:Users/Dumi/Library/Application Support/Mendeley Desktop/Downloaded/Ravikumar et al. - 2004 - Inhibition of mTOR induces autophagy and reduces toxicity of polyglutamine expansions in fly and mouse models.pdf:pdf},
