title = {{Neurodegenerative diseases and exposure to pesticides in the elderly}},
url = {https://academic.oup.com/aje/article-abstract/157/5/409/76551},
volume = {157},
year = {2003}
}
@article{Tackenberg2015,
abstract = {Synaptic loss is one of the major features of Alzheimer's disease (AD) and correlates well with the degree of dementia. NMDA receptors (NMDARs) have been shown to mediate downstream effects of beta-amyloid (Abeta). NMDARs can trigger intracellular cascades via Ca{\textless}sup{\textgreater}2+{\textless}/sup{\textgreater} entry, however also Ca{\textless}sup{\textgreater}2+{\textless}/sup{\textgreater}-independent (metabotropic) functions of NMDARs have been described. Here we show that transgenically produced Abeta as well as exogenously added synthetic Abeta oligomers induce dendritic spine loss and reductions in pre- and postsynaptic protein levels in slice cultures. Synaptic alterations were mitigated by blocking glutamate-binding to NMDARs using NMDAR-antagonist APV, but not by preventing ion-flux with Ca{\textless}sup{\textgreater}2+{\textless}/sup{\textgreater}-chelator BAPTA or open channel blocker MK-801 or memantine. Spine loss was mediated by active p38 MAPK. Abeta-induced p38 activation was reduced by APV but not by BAPTA, MK-801 or memantine treatment highlighting the role of glutamate binding to NMDAR but not Ca{\textless}sup{\textgreater}2+{\textless}/sup{\textgreater}-flux for synaptic degeneration. Our data suggest that Abeta-induced synaptic loss is signaled through metabotropic-like activation of NMDARs. This Ca{\textless}sup{\textgreater}2+{\textless}/sup{\textgreater}-independent NMDAR signaling may represent a therapeutic target for drugs designed to reduce synaptic deficits in AD.},
author = {Tackenberg, C and Birnbaum, J H and Nitsch, R M},
doi = {http://dx.doi.org/10.1159/000381736},
file = {:Users/Dumi/Library/Application Support/Mendeley Desktop/Downloaded/Tackenberg, Birnbaum, Nitsch - 2015 - Calcium-flux-independent NMDA receptor activity is required for abeta oligomer-induced synaptic lo.pdf:pdf},
issn = {1660-2854},
journal = {Neurodegenerative Diseases},
keywords = {calcium transport Parkinson disease dementia slice},
number = {6},
pages = {522},
publisher = {Nature Publishing Group},
title = {{Calcium-flux-independent NMDA receptor activity is required for abeta oligomer-induced synaptic loss}},
url = {http://ovidsp.ovid.com/ovidweb.cgi?T=JS{\&}CSC=Y{\&}NEWS=N{\&}PAGE=fulltext{\&}D=emed13{\&}AN=71853494 http://bf4dv7zn3u.search.serialssolutions.com.myaccess.library.utoronto.ca/?url{\_}ver=Z39.88-2004{\&}rft{\_}val{\_}fmt=info:ofi/fmt:kev:mtx:journal{\&}rfr{\_}id=info:sid/Ovid:emed13{\&}rf},
volume = {15},
year = {2015}
}
@article{Wicker1990,
abstract = {The effects of the immunosuppressants cyclosporin A (CsA), FK506 and rapamycin have been compared using murine B cells activated with a variety of mitogens. FK506 is a macrolide antibiotic that has been recently shown to inhibit T cell activation by a mechanism that appears similar to that of CsA. Rapamycin is a macrolide structurally related to FK506 whose mechanism of T cell suppression appears to be distinct from that of FK506 and CsA. While CsA and FK506 were found to preferentially inhibit B cell activation caused by stimuli which induce a rise in intracellular calcium, rapamycin partially inhibited activation by all stimuli tested, including those which are not associated with a calcium flux. All three compounds were found to inhibit cell cycle progression within the G1 phase; however, the rapamycin-sensitive event within G1 was completed earlier than the G1 events inhibited by CsA and FK506. In addition, inhibition of anti-IgM-activated B cells with CsA and FK506, but not with rapamycin, resulted in cell death. These data suggest that although CsA, FK506 and rapamycin are all inhibitors of B cell activation, the inhibitory activity of rapamycin can be clearly distinguished from that of CsA and FK506. Although the suppressive effects of CsA and FK506 on B cell proliferation were nearly identical in this study, their biological activities were distinguishable since FK506, but not CsA, could antagonize rapamycin-mediated suppression.},
author = {Wicker, Linda S. and Boltz, Robert C. and Matt, Victoria and Nichols, Elizabeth A. and Peterson, Laurence B. and Sigal, Nolan H.},
doi = {10.1002/eji.1830201017},
issn = {00142980},
journal = {European Journal of Immunology},
month = {oct},
number = {10},
pages = {2277--2283},
pmid = {1700753},
title = {{Suppression of B cell activation by cyclosporin A, FK506 and rapamycin}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/1700753 http://doi.wiley.com/10.1002/eji.1830201017},
volume = {20},
year = {1990}
}
@article{Chen2015,
abstract = {Exposure to environmental toxins such as pesticides is implicated in increasing Alzheimer's disease risk. In this study, we investigated the long-term effects of paraquat exposure on cognition of Alzheimer's disease animal model APP/PS1 mice and wild-type (WT) mice. Our results showed that APP/PS1 mice had exacerbated cognition impairment and elevated A$\beta$ levels at 5months after paraquat exposure, and that WT mice had cognition impairment at 5 and 16months after paraquat exposure. In addition, increased mitochondrial oxidative stress and augmented brain inflammation were observed in both paraquat-exposed APP/PS1 mice and WT mice. Interestingly, activation of NLRP3 inflammasome, which triggers inflammation in response to mitochondrial stress, was enhanced in paraquat-exposed mice. Moreover, transgenic mice overexpressing Prdx3, a key enzyme in detoxifying mitochondrial H{\textless}inf{\textgreater}2{\textless}/inf{\textgreater}O{\textless}inf{\textgreater}2{\textless}/inf{\textgreater}, had suppressed NLRP3 inflammasome activation, reduced brain inflammation, and attenuated cognition impairment after paraquat exposure. Together, our results indicate that NLRP3 inflammasome activation induced by mitochondrial reactive oxygen species plays a key role in mediating paraquat-induced long-term cognition decline by elevating brain inflammation.},
author = {Chen, Liuji and Na, Ren and Boldt, Erin and Ran, Qitao},
doi = {10.1016/j.neurobiolaging.2015.05.018},
file = {:Users/Dumi/Library/Application Support/Mendeley Desktop/Downloaded/Chen et al. - 2015 - NLRP3 inflammasome activation by mitochondrial reactive oxygen species plays a key role in long-term cognitive impa.pdf:pdf},
issn = {15581497},
journal = {Neurobiology of Aging},
keywords = {Alzheimer's disease,A$\beta$,Inflammation,Mitochondria,NLRP3 inflammasome,Oxidative stress,Paraquat,Toxin exposure},
month = {sep},
number = {9},
pages = {2533--2543},
publisher = {Elsevier Inc.},
title = {{NLRP3 inflammasome activation by mitochondrial reactive oxygen species plays a key role in long-term cognitive impairment induced by paraquat exposure}},
volume = {36},
year = {2015}
}
