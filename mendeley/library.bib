title = {{Decrease in Polyamines with Aging and Their Ingestion from Food and Drink}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16428322 http://academic.oup.com/jb/article/139/1/81/1050555/Decrease-in-Polyamines-with-Aging-and-Their},
