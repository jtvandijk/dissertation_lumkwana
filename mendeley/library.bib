issn = {1097-4172},
journal = {Cell},
keywords = {Alzheimer Disease,Alzheimer Disease: metabolism,Alzheimer Disease: pathology,Animals,Autophagy,Blastocyst,Blastocyst: metabolism,CELLBIO,Cell Line,Gene Deletion,Gene Knockout Techniques,Glycosylation,HUMDISEASE,Humans,Hydrolysis,Knockout,Lysosomes,Lysosomes: metabolism,Mice,Neurons,Neurons: metabolism,Presenilin-1,Presenilin-1: genetics,Presenilin-1: metabolism,Proteins,Proteins: metabolism,Vacuolar Proton-Translocating ATPases,Vacuolar Proton-Translocating ATPases: metabolism,Vacuoles,Vacuoles: metabolism},
