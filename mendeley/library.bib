file = {:Users/Dumi/Library/Application Support/Mendeley Desktop/Downloaded/Salminen et al. - 2013 - Impaired autophagy and APP processing in Alzheimer's disease The potential role of Beclin 1 interactome.pdf:pdf},
isbn = {1873-5118 (Electronic)$\backslash$r0301-0082 (Linking)},
issn = {03010082},
journal = {Progress in Neurobiology},
keywords = {Ageing,Alzheimer's disease,Autophagy,Beclin 1,Mitophagy,Vps34},
pages = {33--54},
pmid = {23827971},
publisher = {Elsevier Ltd},
title = {{Impaired autophagy and APP processing in Alzheimer's disease: The potential role of Beclin 1 interactome}},
