isbn = {1873-6815 (Electronic)$\backslash$n0531-5565 (Linking)},
issn = {05315565},
journal = {Experimental Gerontology},
keywords = {Glomerulosclerosis,Longevity,Mice,Polyamine,SMP-30,Spermidine,Spermine},
number = {11},
pages = {727--732},
pmid = {19735716},
publisher = {Elsevier Inc.},
title = {{Polyamine-rich food decreases age-associated pathology and mortality in aged mice}},
