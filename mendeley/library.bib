journal = {Journal of Alzheimer's disease : JAD},
number = {0 1},
pages = {S405--16},
pmid = {22695623},
publisher = {NIH Public Access},
title = {{Characterizing the preclinical stages of Alzheimer's disease and the prospect of presymptomatic intervention.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22695623 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3628721},
volume = {33 Suppl 1},
year = {2013}
}
