@article{Mueller-Steiner2006,
abstract = {Alzheimer's disease (AD) may result from the accumulation of amyloid-?? (A??) peptides in the brain. The cysteine protease cathepsin B (CatB) is associated with amyloid plaques in AD brains and has been suspected to increase A?? production. Here, we demonstrate that CatB actually reduces levels of A?? peptides, especially the aggregation-prone species A??1-42, through proteolytic cleavage. Genetic inactivation of CatB in mice with neuronal expression of familial AD-mutant human amyloid precursor protein (hAPP) increased the relative abundance of A??1-42, worsening plaque deposition and other AD-related pathologies. Lentivirus-mediated expression of CatB in aged hAPP mice reduced preexisting amyloid deposits, even thioflavin S-positive plaques. Under cell-free conditions, CatB effectively cleaved A??1-42, generating C-terminally truncated A?? peptides that are less amyloidogenic. Thus, CatB likely fulfills antiamyloidogenic and neuroprotective functions. Insufficient CatB activity might promote AD; increasing CatB activity could counteract the neuropathology of this disease. ?? 2006 Elsevier Inc. All rights reserved.},
author = {Mueller-Steiner, Sarah and Zhou, Yungui and Arai, Hideaki and Roberson, Erik D. and Sun, Binggui and Chen, Jennifer and Wang, Xin and Yu, Guiqiu and Esposito, Luke and Mucke, Lennart and Gan, Li},
