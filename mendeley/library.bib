file = {:Users/Dumi/Library/Application Support/Mendeley Desktop/Downloaded/Swanlund, Kregel, Oberley - 2010 - Investigating autophagy Quantitative morphometric analysis using electron microscopy.pdf:pdf},
isbn = {2122633255},
issn = {15548627},
journal = {Autophagy},
keywords = {Autophagy,Electron microscopy,Immunogold,Morphometry,Ultrastructure},
number = {2},
pages = {270--277},
pmid = {19923921},
title = {{Investigating autophagy: Quantitative morphometric analysis using electron microscopy}},
volume = {6},
