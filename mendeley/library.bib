pmid = {22169203},
publisher = {Elsevier Inc.},
title = {{Autophagic degradation of tau in primary neurons and its enhancement by trehalose}},
url = {http://dx.doi.org/10.1016/j.neurobiolaging.2011.11.009},
volume = {33},
year = {2012}
