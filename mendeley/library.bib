file = {:Users/Dumi/Library/Application Support/Mendeley Desktop/Downloaded/Ravikumar et al. - 2010 - Regulation of Mammalian Autophagy in Physiology and Pathophysiology.pdf:pdf},
issn = {0031-9333},
