isbn = {1873-5118 (Electronic)$\backslash$r0301-0082 (Linking)},
issn = {03010082},
journal = {Progress in Neurobiology},
keywords = {Mitochondria dysfunction,Porteopathies,RSVA314,RSVA405,Resveratrol,Trehalose},
month = {jan},
