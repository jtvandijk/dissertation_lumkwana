@article{LaRocca2013a,
abstract = {Arterial aging, characterized by stiffening of large elastic arteries and the development of arterial endothelial dysfunction, increases cardiovascular disease (CVD) risk. We tested the hypothesis that spermidine, a nutrient associated with the anti-aging process autophagy, would improve arterial aging. Aortic pulse wave velocity (aPWV), a measure of arterial stiffness, was {\~{}}20{\%} greater in old (O, 28 months) compared with young C57BL6 mice (Y, 4 months, P{\textless} 0.05). Arterial endothelium-dependent dilation (EDD), a measure of endothelial function, was {\~{}}25{\%} lower in O (P{\textless} 0.05 vs. Y) due to reduced nitric oxide (NO) bioavailability. These impairments were associated with greater arterial oxidative stress (nitrotyrosine), superoxide production, and protein cross-linking (advanced glycation end-products, AGEs) in O (all P{\textless} 0.05). Spermidine supplementation normalized aPWV, restored NO-mediated EDD and reduced nitrotyrosine, superoxide, AGEs and collagen in O. These effects of spermidine were associated with enhanced arterial expression of autophagy markers, and in vitro experiments demonstrated that vascular protection by spermidine was autophagy-dependent. Our results indicate that spermidine exerts a potent anti-aging influence on arteries by increasing NO bioavailability, reducing oxidative stress, modifying structural factors and enhancing autophagy. Spermidine may be a promising nutraceutical treatment for arterial aging and prevention of age-associated CVD. ?? 2013 Elsevier Ireland Ltd.},
author = {LaRocca, Thomas J. and Gioscia-Ryan, Rachel A. and Hearon, Christopher M. and Seals, Douglas R.},
doi = {10.1016/j.mad.2013.04.004},
file = {:Users/Dumi/Library/Application Support/Mendeley Desktop/Downloaded/LaRocca et al. - 2013 - The autophagy enhancer spermidine reverses arterial aging.pdf:pdf;:Users/Dumi/Library/Application Support/Mendeley Desktop/Downloaded/LaRocca et al. - 2013 - The autophagy enhancer spermidine reverses arterial aging(2).pdf:pdf},
issn = {00476374},
journal = {Mechanisms of Ageing and Development},
keywords = {Arterial stiffness,Endothelial dysfunction,Nitric oxide,Oxidative stress},
number = {7-8},
