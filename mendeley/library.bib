@article{Ericsson1969,
abstract = {Lysosomes in renal proximal tubule cells and hepatocytes were labelled with electron dense markers. The relationship between such preexisting labelled lysosomes, and forming and early cytosegresomes (autophagic vacuoles, cytolysosomes), was studied in cells subjected to autophagy-inducing treatments (i.p. injected bacitracin for renal tubule cells; partial hepatectomy and i.p. injected glucagon for hepatocytes). Newly formed cytosegresomes containing unaltered organelles were spatially unrelated to preexisting labelled lysosomes and appeared to represent "pre-lysosomes" of the autophagosome type lacking lysosomal enzyme. Later, the apparent autophagosomes seemed to fuse with preexisting labelled lysosomes. The findings are compatible with autophagosome-lysosome fusion as the mechanism whereby lysosomal enzymes are brought in contact with sequestered cytoplasmic organelles for subsequent digestion. {\textcopyright} 1969.},
author = {Ericsson, J. L.E.},
doi = {10.1016/0014-4827(69)90462-5},
issn = {00144827},
journal = {Experimental Cell Research},
number = {1},
pages = {95--106},
title = {{Studies on induced cellular autophagy. I. Electron microscopy of cells with in vivo labelled lysosomes}},
volume = {55},
year = {1969}
}
@article{Butler2007,
abstract = {In order to determine the role of subregions of the hippocampus in spatial working memory, this study combined selective neurotoxic lesions of the hippocampal subregions with a simple delayed nonmatching-to-place task on a radial maze in rats. Lesions of the dentate gyrus or the CA3, but not the CA1, subregion of the hippocampus induced a deficit in the acquisition of the task with short-term delays (i.e., 10 sec) and impaired performance of the task in a novel environment. All subregional lesions produced sustained impairment in performing the task with intermediate-term delays (i.e., 5 min) when rats were tested in a familiar environment. The results suggest a dynamic interaction among the dorsal hippocampal subregions in processing spatial working memory, with the time window (i.e., delay) of a task recognized as an essential controlling factor.},
