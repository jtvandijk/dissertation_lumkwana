issn = {00068969},
keywords = {Alzheimer disease,Amyotrophic lateral sclerosis,Glial inflammatory response,Neurodegenerative disease,Neuron death,Oxidative stress,Parkinson disease},
number = {2},
pages = {157--170},
title = {{The role for oxidative stress in neurodegenerative diseases}},
url = {https://synapse.koreamed.org/DOIx.php?id=10.5607/en.2015.24.4.325},
volume = {60},
year = {2008}
}
@article{Cai2012,
abstract = {Autophagy plays a critical role in multiple pathological lesions of Alzheimer's disease (AD), such as the formation of amyloid plaques from amyloid-$\beta$ (A$\beta$) production and accumulation via dysregulating amyloid precursor protein turnover and enhancing the activity of $\beta$- and/or $\gamma$-secretases, intraneuronal neurofibrillary tangles (NFT) because of tau hyperphosphorylation, and neuronal apoptosis. Dysfunction of the autophagy-lysosome system also contributes to A$\beta$ accumulation and the formation of tau oligomers and insoluble aggregates, because induction of autophagy enhances the clearance of both soluble and aggregated forms of A$\beta$ and tau proteins. The mammalian target of rapamycin (mTOR) pathway plays a central role in controlling protein homeostasis and negatively regulates autophagy. Inhibition of mTOR by rapamycin improves cognitive deficits and rescues A$\beta$ pathology and NFTs by increasing autophagy. Several mTOR signaling components may be potential biomarkers of cognitive impairment in the clinical diagnosis of AD. Thus, mTOR-related agents through the control of autophagy-lysosome protein degradation are emerging as an important therapeutic target for AD.},
author = {Cai, Zhiyou and Zhao, Bin and Li, Keshen and Zhang, Liangqing and Li, Chunhua and Quazi, Sohel H. and Tan, Yan},
doi = {10.1002/jnr.23011},
file = {:Users/Dumi/Library/Application Support/Mendeley Desktop/Downloaded/Cai et al. - 2012 - Mammalian target of rapamycin A valid therapeutic target through the autophagy pathway for alzheimer's disease.pdf:pdf},
