issn = {15514005},
journal = {Cell Cycle},
keywords = {Aging,Autophagy,Dopaminergic neuron loss,Motor dysfunction,Neurodegeneration,Parkinson's disease,Spermidine,$\alpha$-synuclein},
month = {dec},
number = {24},
pages = {3903--3908},
pmid = {25483063},
title = {{Spermidine protects against $\alpha$-synuclein neurotoxicity}},
url = {http://www.tandfonline.com/action/journalInformation?journalCode=kccy20 https://www.tandfonline.com/doi/pdf/10.4161/15384101.2014.973309?needAccess=true http://www.tandfonline.com/doi/full/10.4161/15384101.2014.973309},
volume = {13},
