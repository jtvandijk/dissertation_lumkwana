journal = {Acta neuropathologica},
keywords = {10,14,3,31,33,34,9,alzheimer,amyloid - neurofibrillary changes,but shows a characteristic,d e m e,n t i a,pattern,s disease - stageing},
number = {4},
pages = {239--259},
pmid = {1759558},
title = {{Neuropathological stageing of Alzheimer-related changes.}},
url = {http://link.springer.com/10.1007/BF00308809},
