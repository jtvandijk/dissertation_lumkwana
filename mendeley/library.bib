file = {:Users/Dumi/Library/Application Support/Mendeley Desktop/Downloaded/Ferri et al. - 2005 - Global prevalence of dementia a Delphi consensus study.pdf:pdf},
issn = {01406736},
journal = {The Lancet},
month = {dec},
number = {9503},
