issn = {15264602},
journal = {Biomacromolecules},
number = {7},
title = {{Synthesis and Cell Interaction of Statistical l -Arginine-Glycine- l -Aspartic Acid Terpolypeptides}},
volume = {19},
year = {2018}
}
