journal = {Neurobiology of Learning and Memory},
month = {nov},
number = {4},
pages = {574--580},
publisher = {Academic Press},
title = {{Spermine improves recognition memory deficit in a rodent model of Huntington's disease}},
url = {https://www.sciencedirect.com/science/article/pii/S1074742709001439?via{\%}3Dihub},
volume = {92},
year = {2009}
}
