pages = {15024--15029},
pmid = {22932872},
publisher = {National Academy of Sciences},
title = {{Autophagy activators rescue and alleviate pathogenesis of a mouse model with proteinopathies of the TAR DNA-binding protein 43}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22932872 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3443184 http://www.pnas.org/cgi/doi/10.1073/pnas.1206362109},
volume = {109},
