title = {{Spermidine in health and disease}},
url = {http://science.sciencemag.org/ http://www.ncbi.nlm.nih.gov/pubmed/29371440 http://www.sciencemag.org/lookup/doi/10.1126/science.aan2788},
volume = {359},
year = {2018}
