file = {:Users/Dumi/Library/Application Support/Mendeley Desktop/Downloaded/McCormack et al. - 2002 - Environmental risk factors and Parkinson's disease Selective degeneration of nigral dopaminergic neurons cause.pdf:pdf},
issn = {09699961},
journal = {Neurobiology of Disease},
keywords = {Dopamine,GABA,Hippocampus,Mice,Neurotoxicity,Pesticide,Stereology,Striatum,Substantia nigra},
number = {2},
