	Journal = {Toxicological Sciences},
	Keywords = {Apoptosis,Paraquat,Parkinson,Vacuoles},
	Number = {2},
	Pages = {448--458},
	Publisher = {Oxford University Press},
	Tissn = {1096-6080},
	Title = {{Inhibition of Paraquat-Induced Autophagy Accelerates the Apoptotic Cell Death in Neuroblastoma SH-SY5Y Cells}},
	Volume = {97},
	Year = {2007},
	Bdsk-Url-1 = {https://academic.oup.com/toxsci/article-lookup/doi/10.1093/toxsci/kfm040},
