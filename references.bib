	Journal = {Molecular Medicine},
	Number = {7-8},
	Pages = {275--282},
	Tissn = {1076-1551},
	Title = {{Spermine Protects Mice Against Lethal Sepsis Partly by Attenuating Surrogate Inflammatory Markers}},
	Volume = {15},
