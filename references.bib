	Doi = {10.1007/s00726-007-0513-4},
	File = {:Users/Dumi/Library/Application Support/Mendeley Desktop/Downloaded/Rider et al. - 2007 - Spermine and spermidine mediate protection against oxidative damage caused by hydrogen peroxide.pdf:pdf},
	Journal = {Amino Acids},
	Keywords = {Antioxidants,Hydrogen peroxide,Oxidative damage,Polyamines,Spermidine,Spermine},
	Number = {2},
	Pages = {231--240},
	Tissn = {0939-4451},
	Title = {{Spermine and spermidine mediate protection against oxidative damage caused by hydrogen peroxide}},
	Volume = {33},
	Year = {2007},
	Bdsk-Url-1 = {http://link.springer.com/10.1007/s00726-007-0513-4},
