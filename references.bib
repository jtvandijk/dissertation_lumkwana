	Isbn = {1945-4589 (Electronic)$\backslash$r1945-4589 (Linking)},
	Journal = {Aging},
	Keywords = {1,2 homology,2 $\alpha$,4,5,abbreviations,activated protein kinase,amp,ampk,atg,autophagy,bcl,bh3,caenorhabditis elegans,double,dsrna,eif2 $\alpha$,endoplasmic reticulum,er,erk,eukaryotic translation initiation factor,extracellular signal,hif,hypoxia,i $\kappa$ b kinase,igf,ikk,inducible factor 1,inositol 1,insulin,ip 3,ip 3 r,like growth factor,mtor,p53,regulated kinase,related,rna,saccharomyces cerevisiae,stranded,trisphosphate},
