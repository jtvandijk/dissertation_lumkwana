	File = {:Users/Dumi/Library/Application Support/Mendeley Desktop/Downloaded/HE et al. - 1993 - Correlation between the inhibition of cell growth by accumulated polyamines and the decrease of magnesium and ATP.pdf:pdf},
	Journal = {European Journal of Biochemistry},
	Number = {1},
	Pages = {89--96},
	Tissn = {14321033},
	Title = {{Correlation between the inhibition of cell growth by accumulated polyamines and the decrease of magnesium and ATP}},
	Volume = {217},
	Year = {1993},
