	File = {:Users/Dumi/Library/Application Support/Mendeley Desktop/Downloaded/Colangelo, Alberghina, Papa - 2014 - Astrogliosis as a therapeutic target for neurodegenerative diseases.pdf:pdf},
	Journal = {Neuroscience Letters},
	Keywords = {Brain homeostasis,Neuroprotection,Neurotrophins,Reactive astrogliosis,Systems biology},
