	Isbn = {1554-8635 (Electronic)$\backslash$n1554-8627 (Linking)},
	Journal = {Autophagy},
	Keywords = {Acetylation,Ageing,Aging,Autophagy,Epigenetic,Histone,Longevity,Polyamine,Spermidine},
	Pages = {160--162},
	Pmid = {20110777},
	Tissn = {15548627},
	Title = {{Spermidine: A novel autophagy inducer and longevity elixir}},
	Volume = {6},
	Year = {2010},
	Bdsk-Url-1 = {https://doi.org/10.4161/auto.6.1.10600}}

@article{Bhukel2017,
	Abstract = {All animals form memories to adapt their behavior in a context-dependent manner. With increasing age, however, forming new memories becomes less efficient. While synaptic plasticity promotes memory formation, the etiology of age-induced memory formation remained enigmatic. Previous work showed that simple feeding of polyamine spermidine protects from age-induced memory impairment in Drosophila. Most recent work now shows that spermidine operates directly at synapses, allowing for an autophagy-dependent homeostatic regulation of presynaptic specializations. How exactly autophagic regulations intersect with synaptic plasticity should be an interesting subject for future research.},
	Author = {Bhukel, Anuradha and Madeo, Frank and Sigrist, Stephan J.},
