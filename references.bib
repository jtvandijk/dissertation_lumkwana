	Journal = {Free Radical Biology and Medicine},
	Keywords = {AGN 192403,Antimycin A,Astrocytes,Caspase-9,Cytochrome c,Free radicals,Guanabenz,Lysosomal stability,Mitochondrial potential,Naphthazarin,Rilmenidine,Rotenone},
