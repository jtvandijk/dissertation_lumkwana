	File = {:Users/Dumi/Library/Application Support/Mendeley Desktop/Downloaded/Park et al. - 2011 - Protective effect of the phosphodiesterase III inhibitor cilostazol on amyloid $\beta$-induced cognitive deficits assoc.pdf:pdf},
	Journal = {Biochemical and Biophysical Research Communications},
	Keywords = {Alzheimer's disease,Amyloid $\beta$,ApoE,Microglia,Phosphorylated tau},
