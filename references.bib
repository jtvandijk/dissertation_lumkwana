	Doi = {10.1046/j.1471-4159.2001.00096.x},
	File = {:Users/Dumi/Library/Application Support/Mendeley Desktop/Downloaded/Chun et al. - 2001 - Dopaminergic cell death induced by MPP, oxidant and specific neurotoxicants shares the common molecular mechanis(2).pdf:pdf},
	Journal = {Journal of Neurochemistry},
	Keywords = {Caspase 1,Caspase 3,Dopaminergic cell death,Heme oxygenase-1,Oxidative stress,Parkinson's disease,ROS,c-Jun N-terminal kinase (JNK),cDNA microarray},
	Number = {4},
