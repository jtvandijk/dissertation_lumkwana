	Doi = {10.1016/j.ultramic.2013.10.006},
	File = {:Users/Dumi/Library/Application Support/Mendeley Desktop/Downloaded/Duke et al. - 2014 - Imaging endosomes and autophagosomes in whole mammalian cells using correlative cryo-fluorescence and cryo-soft (3).pdf:pdf},
	Journal = {Ultramicroscopy},
	Keywords = {Autophagosomes,Cryo-CLXM,Cryo-fluorescence,Cryo-soft X-ray tomography,Endosomes,Omegasomes},
	Pages = {77--87},
	Publisher = {Elsevier},
	Tissn = {18792723},
	Title = {{Imaging endosomes and autophagosomes in whole mammalian cells using correlative cryo-fluorescence and cryo-soft X-ray microscopy (cryo-CLXM)}},
