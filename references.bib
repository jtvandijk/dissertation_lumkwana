	Doi = {10.1089/ars.2010.3486},
	File = {:Users/Dumi/Library/Application Support/Mendeley Desktop/Downloaded/Petrovski et al. - 2011 - Cardioprotection by endoplasmic reticulum stress-induced autophagy.pdf:pdf},
	Journal = {Antioxidants {\&} Redox Signaling},
