	Doi = {10.12659/MSM.898679},
	File = {:Users/Dumi/Library/Application Support/Mendeley Desktop/Downloaded/Zhang et al. - 2017 - Evaluating the effectiveness of GTM-1, rapamycin, and carbamazepine on autophagy and Alzheimer disease.pdf:pdf},
	TISSN = {16433750},
	Journal = {Medical Science Monitor},
	Keywords = {Alzheimer disease,Autophagy,Carbamazepine,TOR serine-threonine kinases},
	Pages = {801--808},
	Publisher = {International Scientific Literature Inc.},
	Title = {{Evaluating the effectiveness of GTM-1, rapamycin, and carbamazepine on autophagy and Alzheimer disease}},
	Volume = {23},
	Year = {2017},
