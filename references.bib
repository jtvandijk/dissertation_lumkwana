	TISSN = {01974580},
	Journal = {Neurobiology of Aging},
	Keywords = {Alzheimer's disease,Autophagy,Degradation,Tau protein},
	Number = {10},
	Pages = {2291--2305},
	Title = {{Autophagic degradation of tau in primary neurons and its enhancement by trehalose}},
