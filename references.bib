	Journal = {Autophagy},
	Keywords = {Amphisome,Autolysosome,Autophagosome,Cytochemistry,Electron microscopy,Electron tomography,Freeze-fixation,Freeze-fracture,Freeze-substitution,Immunoelectron microscopy,Immunogold labeling,Morphology,PAS,Phagophore,Ultrastructure},
	Number = {9},
